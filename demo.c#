class hello
{
    int a,b;
    public void sm()
    {
      Console.WriteLine("Hello Some");
    }
    public void A()
    {
      Console.WriteLine("Print A");
    }
    public void B()
    {
      Console.WriteLine("Print B");
    }
    public void C()
    {
      Console.WriteLine("Print C");
    }
    public void funInsertCodes()
    {
      Console.WriteLine("Print A+B");
    }
    public void funDeleteCodes()
    {
      Console.WriteLine("Codes Deleted");
    }
    public void funUpdateCodes()
    {
      Console.WriteLine("Codes Updated");
    }
}
class test
{
  public static void main()
  {
    hello h = new hello();
    h.sm();
    h.A();
    h.B();
    h.C();
    h.funInsertCodes();
    h.funDeleteCodes();
    h.funUpdateCodes();
  }
}
